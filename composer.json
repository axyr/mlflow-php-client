{
    "name": "martijn/mlflow-php-client",
    "description": "A Laravel package for MLflow REST API with complete tracking, model registry, and experiment management",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "mlflow", "machine-learning", "ml-tracking", "model-registry", "experiment-tracking"],
    "authors": [
        {
            "name": "Martijn",
            "email": "info@axyrmedia.nl"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.0",
        "psr/log": "^1.1|^2.0|^3.0",
        "psr/simple-cache": "^2.0|^3.0",
        "illuminate/support": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/log": "^12.0",
        "illuminate/config": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.18",
        "phpstan/phpstan": "^2.0",
        "larastan/larastan": "^3.0",
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "MLflow\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MLflow\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpstan": "phpstan analyse",
        "pint": "pint",
        "pint:test": "pint --test",
        "quality": [
            "@pint:test",
            "@phpstan",
            "@test"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MLflow\\Laravel\\MLflowServiceProvider"
            ],
            "aliases": {
                "MLflow": "MLflow\\Laravel\\Facades\\MLflow"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
